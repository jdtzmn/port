{
  "name": "@jdtzmn/port",
  "version": "0.1.1",
  "description": "CLI tool for managing git worktrees with automatic Traefik configuration",
  "author": "Jacob Daitzman <jdtzmn@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdtzmn/port.git"
  },
  "homepage": "https://github.com/jdtzmn/port#readme",
  "bugs": {
    "url": "https://github.com/jdtzmn/port/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "module": "dist/index.js",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "port": "./dist/index.js"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --splitting",
    "typecheck": "bunx tsc --noEmit",
    "dev": "bun run src/index.ts",
    "lint": "bunx eslint .",
    "lint:fix": "bunx eslint . --fix",
    "format": "bunx prettier --write .",
    "format:check": "bunx prettier --check .",
    "test": "bunx vitest",
    "test:tui": "bun test src/tui/__tests__/"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1",
  "dependencies": {
    "@opentui/core": "latest",
    "@opentui/react": "latest",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "fastest-levenshtein": "^1.0.16",
    "inquirer": "^9.2.0",
    "jsonc-parser": "^3.2.0",
    "react": "^19.0.0",
    "simple-git": "^3.20.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/bun": "latest",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "@types/prettier": "^3.0.0",
    "@types/react": "^19.2.14",
    "cli-testing-library": "^3.0.1",
    "eslint": "^9.0.0",
    "jiti": "^2.6.1",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^7.0.0",
    "vitest": "latest"
  }
}
