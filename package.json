{
  "name": "@jdtzmn/port",
  "version": "0.1.0",
  "description": "CLI tool for managing git worktrees with automatic Traefik configuration",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "port": "./dist/index.js"
  },
  "scripts": {
    "build": "bun build src/index.ts --outfile dist/index.js --target bun",
    "typecheck": "tsc --noEmit",
    "dev": "bun run src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest"
  },
  "dependencies": {
    "simple-git": "^3.20.0",
    "docker-compose": "^1.2.0",
    "dockerode": "^4.0.0",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "yaml": "^2.3.0",
    "jsonc-parser": "^3.2.0",
    "inquirer": "^9.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/bun": "latest",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "@types/prettier": "^3.0.0",
    "cli-testing-library": "^3.0.1",
    "eslint": "^9.0.0",
    "jiti": "^2.6.1",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^7.0.0",
    "vitest": "latest"
  }
}
